# Example .env for DashLogistics
# Database (use Supabase or local Postgres)
DATABASE_URL=postgresql://postgres:admin@localhost:5432/shipping_db

# OpenWeather API key
OPENWEATHER_API_KEY=your_openweather_api_key_here

# If you use Supabase (managed), set these values instead of DATABASE_URL
# SUPABASE_URL=https://<your-project>.supabase.co
# SUPABASE_KEY=<your-supabase-service-key>

# Optional fixed run id (if empty, pipeline generates a new one each run)
# PIPELINE_RUN_ID=

# Data paths
RAW_DATA_PATH=data/raw/shipping_data.csv
CLEAN_DATA_PATH=data/clean/shipping_data_clean.csv
